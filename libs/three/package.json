{
  "name": "three_jsroot",
  "version": "0.145.0",
  "description": "JavaScript 3D library",
  "main": "Three_jsroot.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrdoob/three.js"
  },
  "sideEffects": false,
  "files": [
    "Three_jsroot.js",
    "package.json",
    "README.md"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "mdcs",
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "configFile": "./.babelrc.json"
      }
    },
    "plugins": [
      "html"
    ],
    "globals": {
      "potpack": true,
      "fflate": true,
      "ZSTDDecoder": true,
      "bodymovin": true,
      "OIMO": true,
      "Stats": true,
      "XRWebGLBinding": true,
      "XRWebGLLayer": true
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  },
  "scripts": {
    "build": "patch ../../../threejs/examples/jsm/controls/OrbitControls.js OrbitControls.patch && cp Three_jsroot.js ../../../threejs/src && rollup -c rollup.jsroot.config.js && rm ../../../threejs/src/Three_jsroot.js && git -C ../../../threejs restore examples/jsm/controls/OrbitControls.js"
  },
  "keywords": [
    "three",
    "three.js",
    "javascript",
    "3d",
    "virtual-reality",
    "augmented-reality",
    "webgl",
    "webgl2",
    "webaudio",
    "webgpu",
    "webxr",
    "canvas",
    "svg",
    "html5"
  ],
  "author": "mrdoob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrdoob/three.js/issues"
  },
  "homepage": "https://threejs.org/",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "chalk": "^4.1.2",
    "concurrently": "^6.2.2",
    "eslint": "^7.32.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-plugin-html": "^6.2.0",
    "glob": "^7.2.0",
    "rollup": "^2.57.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "servez": "^1.12.0"
  }
}
