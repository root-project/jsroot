{
    "source": {
      "include": ["modules"],
      "includePattern": ".mjs$",
      "excludePattern": "(node_modules|main.mjs|three.mjs|d3.mjs|dat.gui.mjs)"
    },
    "sourceType": "module",
    "recurseDepth": 10,
    "plugins": [
       "plugins/markdown",
       "plugins/summarize"
    ],
    "templates": {
       "cleverLinks": true,
       "monospaceLinks": true
    },
    "opts": {
       "recurse": true,
       "destination": "./docs/jsdoc/",
       "template": "./node_modules/docdash",
       "cleverLinks": true,
       "monospaceLinks": true,
       "name": "JSROOT",
       "default": {
          "outputSourceFiles": true
       },
       "applicationName": "JavaScript ROOT",
       "disqus": "",
       "googleAnalytics": "",
       "openGraph": {
         "title": "",
         "type": "website",
         "image": "",
         "site_name": "",
         "url": ""
       },
       "meta": {
          "title": "",
          "description": "",
          "keyword": ""
       },
       "linenums": false
    },
    "docdash": {
       "collapse": true,
       "sectionOrder": [
           "Externals",
           "Events",
           "Mixins",
           "Tutorials",
           "Interfaces",
           "Modules",
           "Classes",
           "Namespaces",
           "Global"
        ]
    }
}